
function FirstView(){

var self=Ti.UI.createView(),


label=Ti.UI.createLabel({
color:'#000000',
text:String.format(L('welcome'),'Titanium'),
height:'auto',
width:'auto'});








return self.add(label),label.addEventListener('click',function(e){alert(e.source.text)}),self;
}

module.exports=FirstView;