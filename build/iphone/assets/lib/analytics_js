function analytics(){
var win1=Titanium.UI.createWindow({
title:'Tab 1',
backgroundColor:'#ccc'}),



aButton=Ti.UI.createButton({
title:'aButton',
height:Ti.UI.SIZE,
width:Ti.UI.SIZE});















return aButton.addEventListener('click',function(){alert(Titanium.Analytics.featureEvent('app.feature'))}),win1.add(aButton),win1;
}

module.exports=analytics;