function alertDialog(){
var win1=Titanium.UI.createWindow({
title:'Tab 1',
backgroundColor:'#ccc'}),



aButton=Ti.UI.createButton({
title:'aButton',
height:Ti.UI.SIZE,
width:Ti.UI.SIZE});



















return aButton.addEventListener('click',function(){var alertDialog=Titanium.UI.createAlertDialog({message:'MESSAGE',buttonNames:['OK','Cancel'],persistent:!1,canceledOnTouchOutside:!1});alertDialog.show()}),win1.add(aButton),win1;
}

module.exports=alertDialog;