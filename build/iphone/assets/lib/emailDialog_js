function emailSend(){

var win=Ti.UI.createWindow({
backgroundColor:'green',
layout:'vertical'}),





webview=Titanium.UI.createWebView({
url:'http://www.appcelerator.com',
width:Ti.UI.FILL,
height:400});


win.add(webview);


var email=Ti.UI.createButton({
title:'Opne',
height:100,
width:100,

top:30});








































return email.addEventListener('click',function(e){}),win.add(email),win;

}

module.exports=emailSend;